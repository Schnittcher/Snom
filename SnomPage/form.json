        {
            "type": "Button",
            "caption": "URL for remote rendering",
            "onClick": "SNM_GetRenderRemoteUrl($id);"
        },
        {
            "type": "Label",
            "name": "RenderRemoteUrl",
            "caption": ""
        },
        {
            "type": "Button",
            "caption": "Page content",
            "onClick": "SNM_GetPageContent($id);"
        },
        {
            "type": "Label",
            "name": "PageContent",
            "caption": ""
        }
