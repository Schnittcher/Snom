{
    "elements": [
        {
            "type": "ValidationTextBox",
            "name": "PhoneIP",
            "caption": "Phone IP address"
        },
        {
            "type": "ValidationTextBox",
            "name": "LocalIP",
            "caption": "Local IP address"
        },
        {
            "type": "ValidationTextBox",
            "name": "Message",
            "caption": "Message"
        },
        {
            "type": "NumberSpinner",
            "name": "Timeout",
            "caption": "Page timeout",
            "minimum": 1,
            "suffix": "miliseconds"
        }
