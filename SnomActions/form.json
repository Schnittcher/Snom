                "align": "center",
                "add": 0,
                "edit": {
                    "type": "SelectVariable"
                },
                "onChange": "SNMA_SetVariableId($id, $ActionVariableId);"
            },
            {
                "caption": "Value",
                "name": "ActionValue",
                "width": "auto",
                "align": "center",
                "add": 0,
                "edit": {
                    "type": "SelectValue",
                    "variableID": 0
                }
            }
        ]
        }
    ]
}